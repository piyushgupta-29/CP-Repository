#include<bits/stdc++.h>
using namespace std;

#define ll long long int
#define mod 1000000007
#define vll vector<ll>

int main() {
	ll t,n,i,j,k,x,y,q,m,ans,sum,cnt,mx,mn,l,r,len;
    cin>>t;
	while(t--)
	{
        sum=0;ans=0;cnt=0;mx=-1;mn=1e18;
        cin>>n;
        string s;
        cin>>s;
        for(i=0;i<n;i++)
        {
            if(s[i]=='0')
                sum++;
        }
        if(sum==1)
        {
            cout<<"BOB"<<endl;
            continue;
        }
        if(sum%2)
        {
            cout<<"ALICE"<<endl;
        }
        else
        {
            cout<<"BOB"<<endl;
        }
    }
}
