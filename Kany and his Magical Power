#include<bits/stdc++.h>
using namespace std;
 
#define boost ios_base::sync_with_stdio(false);cin.tie(NULL)
#define endl "\n"
#define ll long long int
#define vll vector<ll>
#define pb push_back
#define mll map<ll,ll>
 a
int main()
{
    boost;
    ll t,n,i,x,y,m,sum,ans,mx,j;
    sum=0;ans=0;mx=INT_MIN;
    cin>>n>>k;
    vll a(n);
    mll mp;
    vll pre(n,0);
    for(i=0;i<n;i++)
    {
        cin>>a[i];
        mp[a[i]]++;
    }
    sort(a.begin(),a.end());
    for(i=1;i<n;i++)
        pre[i]=pre[i-1]+a[i];
    for(i=1;i<n;i++)
    {
        ll l=0,r=i-1;sum=0;
        while(r>=l)
        {
            ll mid=(l+r)/2;
            x=(r-mid+1)*a[i]-(pre[r]-(mid==0?0:pre[mid-1]));
            if(x>k)
            {
                l=mid+1;
            }
            else 
            {
                sum=r-mid+1;
                r=mid-1;
            }
        }
        if(sum+1>mx)
        {
            mx=sum+1;
            ans=a[i];
        }
    }
    cout<<mx<<' '<<ans<<endl;
}
