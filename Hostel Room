#define boost ios_base::sync_with_stdio(false);cin.tie(NULL)
#define endl "\n"
#define ll long long int
#define vll vector<ll>
#define pb push_back
 aaaaaa
int main()a
{
	boost;
	ll t,n,i,x,y,m,sum,ans,cnt,mx,mn,j,k;
	cin>>t;
	while(t--)
	{
		ans=0;mx=INT_MIN;mn=1e18;
		cin>>n>>x;
		vll a(n);
		for(i=0;i<n;i++)	
			cin>>a[i];
		mx=x;
		for(i=0;i<n;i++)
		{
			x+=a[i];
			if(x>mx)
				mx=x;
		}
		cout<<mx<<endl;
	}
}
