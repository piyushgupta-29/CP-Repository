#include<bits/stdc++.h>
using namespace std;
 
#define boost ios_base::sync_with_stdio(false);cin.tie(NULL)
#define endl "\n"
#define ll long long int
#define mod 1000000007
#define vll vector<ll>
#define pb push_back
#define mll map<ll,ll>
#define pll pair<ll,ll>
 
int main()
{
	boost;
	ll t,n,i,x,y,m,p,q,sum,ans,cnt,mx,mn,j,k;
	// cin>>t;
	// while(t--)
	// {
		sum=0;cnt=0;ans=0;mx=INT_MIN;mn=1e18;
		cin>>x;
		cin>>n;
		vll a(n);
		for(i=0;i<n;i++)
			cin>>a[i];
		sort(a.begin(),a.end());
		vll dp(n+1,0);
		dp[0]=0;
		for(i=1;i<=x;i++)
		{
			for(j=0;j<n;j++)
			{
				if(i>=a[j])
				{
					if(dp[i-a[i]]==0)
						dp[i]=1;
				}
			}
		}
		if(dp[x])
			cout<<"ALICE"<<endl;
		else 
			cout<<"BOB"<<endl;
}
