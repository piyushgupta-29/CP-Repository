#include<bits/stdc++.h>
using namespace std;
 
#define boost ios_base::sync_with_stdio(false);cin.tie(NULL)
#define endl "\n"
#define ll long long int
#define vll vector<ll>
#define vvll vector<vll>
#define pb push_back
#define mll map<ll,ll>
#define pll pair<ll,ll>
 
int main()
{
    boost;
    ll t,n,i,x,y,m,p,q,sum,ans,cnt,mx,mn,j,k;
    // cin>>t;
    // while(t--)
    // {
        sum=0;cnt=0;ans=0;mx=INT_MIN;mn=1e18;
        cin>>n;
        cin>>k;
        queue<ll> q1,q2;
        for(i=0;i<k;i++)
        {
            cin>>x;
            q1.push(x);
        }
        cin>>k;
        for(i=0;i<k;i++)
        {
            cin>>x;
            q2.push(x);
        }
        j=0;
        while(j<100000)
        {
            // cout<<q1.front()<<' '<<q2.front()<<endl;
            if(q1.size()==0||q2.size()==0)
                break;
            if(q1.front()>q2.front())
            {
                q1.push(q2.front());
                q1.push(q1.front());
                q1.pop();
                q2.pop();
            }
            else if(q2.front()>q1.front())
            {
                q2.push(q1.front());
                q2.push(q2.front());
                q2.pop();
                q1.pop();
            }
            j++;
        }
        if(j>=100000)
            cout<<-1<<endl;
        else
        {
            if(q1.size()==0)
                cout<<j<<' '<<2<<endl;
            else    
                cout<<j<<' '<<1<<endl;
        }
    // }
}
