                mx1=max(mx1,a[i]*b[j]);
            }
            if(mx1>mx2)
            {
                mx2=mx1;
                k=i;
            }
        }
        for(i=0;i<n;i++)
        {
            if(i==k)
                continue;
            for(j=0;j<m;j++)
                mx=max(mx,a[i]*b[j]);
        }
        cout<<mx<<endl;
