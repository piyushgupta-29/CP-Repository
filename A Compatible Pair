#include<bits/stdc++.h>
using namespace std;
 
#define boost ios_base::sync_with_stdio(false);cin.tie(NULL)
#define endl "\n"
#define ll long long int
#define vll vector<ll>
#define pb push_back
#define mll map<ll,ll>
#define pll pair<ll,ll>
aaaa
int main()
{
    boost;
    ll t,n,i,x,y,m,p,q,sum,ans,cnt,mx,mn,j,k;
        sum=0;cnt=0;ans=0;mx=-1e18;mn=1e18;
        cin>>n>>m;
        vll a(n),b(m);
        for(i=0;i<n;i++)
        {
            cin>>a[i];
            if(a[i]<0)
                cnt++;
            else    
                sum++;
        }
        x=0,y=0;
        for(i=0;i<m;i++)
        {
            cin>>b[i];
            if(b[i]<0)
                x++;
            else 
                y++;
        }
        sort(a.begin(),a.end());
        sort(b.begin(),b.end());
        ll mx1=-1e18,mx2=-1e19;
        for(i=0;i<n;i++)
        {
            mx1=-1e18;
            for(j=0;j<m;j++)
            {
                mx1=max(mx1,a[i]*b[j]);
            }
            if(mx1>mx2)
            {
                mx2=mx1;
                k=i;
            }
        }
        for(i=0;i<n;i++)
        {
            if(i==k)
                continue;
            for(j=0;j<m;j++)
                mx=max(mx,a[i]*b[j]);
        }
        cout<<mx<<endl;
}
