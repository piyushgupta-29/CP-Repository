#include<bits/stdc++.h>
using namespace std;
 
#define boost ios_base::sync_with_stdio(false);cin.tie(NULL)
#define endl "\n"
#define ll long long int
#define mod 1000000007
#define vll vector<ll>
#define vvll vector<vll>
#define pb push_back
#define mll map<ll,ll>
#define pll pair<ll,ll>
 
int main()
{
	boost;
	ll t,n,i,x,y,m,p,q,sum,ans,cnt,mx,mn,j,k;
	cin>>t;
	while(t--)
	{
		sum=0;cnt=0;ans=0;mx=INT_MIN;mn=1e18;
		cin>>n;
		string s;
		cin>>s;
		for(i=0;i<n;i++)	
		{
			if(s[i]=='0')	
				sum++;
		}
		i=0;j=n-1;
		while(j>=i)
		{
			if(s[i]!=s[j])
				cnt++;
			i++;
			j--;
		}
		if(!cnt)
		{
			if(sum==1)
				cout<<"BOB"<<endl;
			else if(sum%2)
				cout<<"ALICE"<<endl;
			else 
				cout<<"BOB"<<endl;
		}
		else 
		{
			if(cnt>=2)
				cout<<"ALICE"<<endl;
			else
			{
				if(sum==2)
					cout<<"DRAW"<<endl;
				else 
					cout<<"ALICE"<<endl;
			}
		}
	}
}
